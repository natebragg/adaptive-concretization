generator int rulefunc2(int x,int y,int z,int bnd){
assert(bnd>0);
if(??) return x;
if(??) return y;
if(??) return z;
if(??) return 0;
if(??) return 1;
int enktd1 = rulefunc2(x,y,z,bnd-1);
int enktd2 = rulefunc2(x,y,z,bnd-1);
if(??) return add(enktd1,enktd2);
if(??) return sub(enktd1,enktd2);
bit cyjze1 = rulefunc1(x,y,z,bnd-1);
if(??) return ite(cyjze1,enktd1,enktd2);
assert false;
}

@Native("(define-fun max3 ((x Int) (y Int) (z Int)) Int")
int max3 (int x,int y,int z){
return rulefunc2(x,y,z,BND);}

harness void main(int z,int y,int x){
assert or(eqInt(x,max3(x,y,z)),or(eqInt(y,max3(x,y,z)),eqInt(z,max3(x,y,z))));
assert gte(max3(x,y,z),z);
assert gte(max3(x,y,z),y);
assert gte(max3(x,y,z),x);
}

