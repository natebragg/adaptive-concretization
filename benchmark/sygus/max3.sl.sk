assert(bnd>0);
int jacnh1 = rulefunc2(x,y,z,bnd-1);
int jacnh2 = rulefunc2(x,y,z,bnd-1);
if(??) return lte(jacnh1,jacnh2);
if(??) return eqInt(jacnh1,jacnh2);
if(??) return gte(jacnh1,jacnh2);
bit xngdq1 = rulefunc1(x,y,z,bnd-1);
if(??) return not(xngdq1);
