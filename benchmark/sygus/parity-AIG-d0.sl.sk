int W = 8;

bit parity(bit a,bit b,bit c,bit d){
return xor(not(xor(a,b)),not(xor(c,d)));
}

generator bit rulefunc1(bit a,bit b,bit c,bit d,int bnd){
assert(bnd>0);
if(??) return a;
if(??) return b;
if(??) return c;
if(??) return d;
bit pgdsb1 = rulefunc1(a,b,c,d,bnd-1);
if(??) return not(pgdsb1);
bit pgdsb2 = rulefunc1(a,b,c,d,bnd-1);
if(??) return and(pgdsb1,pgdsb2);
assert false;
}

@Native("(define-fun AIG ((a Bool) (b Bool) (c Bool) (d Bool)) Bool")
bit AIG (bit a,bit b,bit c,bit d){
return rulefunc1(a,b,c,d,BND);}

harness void main(bit d,bit c,bit b,bit a){
assert eqBit(parity(a,b,c,d),and(AIG(a,b,c,d),not(and(and(not(and(a,b)),not(and(not(a),not(b)))),and(not(and(not(c),not(d))),not(and(c,d)))))));
}

